<div class="panel panel-info">
  <div class="panel-heading" >
    <h1 class="panel-title">
      <span class="glyphicon glyphicon-th-list" aria-hidden="true"></span>
      <%= t('.title')%>
    </h1>
  </div>
  <div class="panel-body row" >
    <% @members.each do |member| %>
      <div id=â€profileâ€ class='col-md-4'>
        <div class="col-md-4">
          <%= gravatar_image_tag(member.email, filetype: :png, rating: 'pg', size: 15, secure: false, gravatar: { default: :identicon } ) %>
        </div>
        <div class="col-md-7">
          <h5><%= link_to member.name, member_path(member) %></h5>
          <dl class="profile">
            <dt>Email:</dt>
            <dd><%= member.email %></dd>
          </dl>
        </div>
      </div>
    <% end %>
    <div class="col-md-12">
      <%= will_paginate @members, :class => 'margin: 0px'%>
    </div>
  </div>
</div>
 





