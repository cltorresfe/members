<div class="panel panel-info">
<div class="panel-heading" > 
  <h1 class="panel-title">
    <span class="glyphicon glyphicon-th-list" aria-hidden="true"></span> 
    <%= t('.title')%>
  </h1>
</div>
<div class="panel-body " >
  <table class ="table table-hover table-striped ">
    <thead>
      <tr>
        <th><%=Member.human_attribute_name("name")%></th>
        <th><%=Member.human_attribute_name("adress")%></th>
        <th><%=Member.human_attribute_name("email")%></th>
        <th><%=Member.human_attribute_name("phone")%></th>
        <th><%=Member.human_attribute_name("church")%></th>
        <th><%=Member.human_attribute_name("status")%></th>
        <th colspan="3"></th>
      </tr>
    </thead>

    <tbody>
      <% @members.each do |member| %>
        <tr>
          <td><%= member.name %></td>
          <td><%= member.adress %></td>
          <td><%= member.email %></td>
          <td><%= member.phone %></td>
          <td><%= member.church.name %></td>
          <td><%= member.status.name %></td>
          <td><%= link_to t('link.show'), member %></td>
          <td><%= link_to t('link.edit'), edit_member_path(member) %></td>
          <td><%= link_to t('link.destroy'), member, method: :delete, data: { confirm: 'Are you sure?' } %></td>
        </tr>
      <% end %>
    </tbody>
  </table>
 <%= will_paginate @members, :class => 'margin: 0px'%>
</div>
</div>




